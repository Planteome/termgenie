[IRIMapping]
localfile: default-ontology-iri-mapping-localfile.settings

[Ontology]
name: GeneOntology
source: http://www.geneontology.org/ontology/obo_format_1_2/gene_ontology_ext.obo
!support: http://www.geneontology.org/scratch/xps/biological_process_xp_self.obo
!support: http://www.geneontology.org/scratch/xps/biological_process_xp_cellular_component.obo
support: http://www.geneontology.org/scratch/xps/molecular_function_xp_protein.obo
!support: http://www.geneontology.org/scratch/xps/biological_process_xp_uber_anatomy.obo
support: http://www.geneontology.org/scratch/xps/biological_process_xp_plant_anatomy.obo
support: http://www.geneontology.org/scratch/xps/cellular_component_xp_protein.obo
requires: ProteinOntology
requires: Uberon
requires: PO ! Plant ontology

[OntologyBranch]
name: biological_process
ontology: GeneOntology
parent: GO:0008150

[OntologyBranch]
name: molecular_function
ontology: GeneOntology
parent: GO:0003674

[OntologyBranch]
name: cellular_component
ontology: GeneOntology
parent: GO:0005575

[Ontology]
name: ProteinOntology
source: ftp://ftp.pir.georgetown.edu/databases/ontology/pro_obo/pro.obo

[Ontology]
name: Uberon
source: http://github.com/cmungall/uberon/raw/master/uberon.obo

[Ontology]
name: HumanPhenotype
source: http://compbio.charite.de/svn/hpo/trunk/src/ontology/human-phenotype-ontology.obo
!support: http://compbio.charite.de/svn/hpo/trunk/src/ontology/human-phenotype-ontology_xp.obo

[Ontology]
name: FMA ! Foundational Model of Anatomy ontology
source: http://obo.svn.sourceforge.net/viewvc/*checkout*/obo/fma-conversion/trunk/fma2_obo.obo

[Ontology]
name: PATO ! Phenotypic Quality Ontology
source: http://pato.googlecode.com/svn/trunk/quality.obo

[Ontology]
name: OMP ! Mammalian Phenotype Ontology
source: http://obo.cvs.sourceforge.net/*checkout*/obo/obo/ontology/phenotype/mammalian_phenotype.obo

[Ontology]
name: CL ! Cell (Type) Ontology
source: http://obo.cvs.sourceforge.net/*checkout*/obo/obo/ontology/anatomy/cell_type/cell.obo

[Ontology]
name: PO ! Plant ontology
source: http://palea.cgrb.oregonstate.edu/viewsvn/Poc/trunk/ontology/OBO_format/po_anatomy.obo?view=co



















