<?xml version="1.0" encoding="UTF-8"?>
<termgenietemplates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="termgenie_rules.xsd">
	<template name="plant_development">
		<description>Development of a plant structure.</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="target" required="true">
				<ontology name="GeneOntology">
					<branch>Plant</branch>
				</ontology>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/plant/plant_development.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>Plant Ontology</category>
			<category>development</category>
		</categories>
	</template>
