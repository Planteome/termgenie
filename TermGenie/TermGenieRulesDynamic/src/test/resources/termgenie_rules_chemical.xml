	<template name="biosynthesis_from">
		<description>biosynthetic process of a chemical entity from a chemical entity</description>
		<obonamespace>biological_process</obonamespace>
		<definition-xref>GO_REF:00000</definition-xref>
		<fields>
			<field name="target" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
			</field>
			<field name="from" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
			</field>
			<field name="Literature_Refs" required="true" >
				<cardinality>1..N</cardinality>
			</field>
			<field name="DefX_Ref" remoteResource="xref">
				<cardinality>1..N</cardinality>
			</field>
			<field name="Comment" label="Optional: public definition comment" hint="&lt;small&gt;This optional text will appear as a definition comment in the ontology and will be visible in GO browsers.&lt;br/&gt;&lt;i&gt;Suggested format&lt;/i&gt;: An example of this is [insert name of gene product, e.g. LysZ] in [insert species name, e.g. E. coli] (UniProt symbol, e.g. UniProt symbol Q13490) in PMID:xxx (inferred from direct assay/mutant phenotype/etc.).&lt;/small&gt;"/>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/chebi/biosynthesis_from.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>chemicals</category>
		</categories>
	</template>
