<?xml version="1.0" encoding="UTF-8"?>
<termgenietemplates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="termgenie_rules.xsd">
	<template name="metabolism_catabolism_biosynthesis">
		<description>Select all three subtemplates to generate
terms for the "metabolic triad". Names, synonyms and definitions are all
generated automatically</description>
		<ontology name="GeneOntology"></ontology>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="target" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
				<prefixes>
					<prefix>metabolism</prefix>
					<prefix>catabolism</prefix>
					<prefix>biosynthesis</prefix>
				</prefixes>
			</field>
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/chebi/metabolism_catabolism_biosynthesis.js</ruleFile>
		</ruleFiles>
	</template>
	<template name="chemical_transport">
		<description>transport of a chemical entity</description>
		<ontology name="GeneOntology"></ontology>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="target" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
			</field>
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/chebi/chemical_transport.js</ruleFile>
		</ruleFiles>
	</template>
	<template name="chemical_binding">
		<description>binding to a chemical entity</description>
		<ontology name="GeneOntology"></ontology>
		<obonamespace>molecular_function</obonamespace>
		<fields>
			<field name="target" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
			</field>
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/chebi/chemical_binding.js</ruleFile>
		</ruleFiles>
	</template>
	<template name="chemical_transmembrane_transport">
		<description>Create trans-membrane transport terms with chemical entities. 
		Select at least one type of genus.</description>
		<ontology name="GeneOntology"></ontology>
		<obonamespace>molecular_function</obonamespace>
		<fields>
			<field name="subject" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
				<prefixes preselected="false">
					<prefix id="GO:0022857">transmembrane transporter activity</prefix>
					<prefix id="GO:0015291">secondary active transmembrane transporter activity</prefix>
					<prefix id="GO:0015563">uptake transmembrane transporter activity</prefix>
					<prefix id="GO:0042626">ATPase activity, coupled to transmembrane movement of substances</prefix>
				</prefixes>
			</field>
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/chebi/transmembrane_transport.js</ruleFile>
		</ruleFiles>
	</template>
	<template name="chemical_response_to">
		<description>response to a chemical entity</description>
		<ontology name="GeneOntology"></ontology>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="target" required="true">
				<ontology name="GeneOntology">
					<branch>chebi</branch>
				</ontology>
			</field>
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/chebi/chemical_response_to.js</ruleFile>
		</ruleFiles>
	</template>
</termgenietemplates>