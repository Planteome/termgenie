<?xml version="1.0" encoding="UTF-8"?>
<termgenietemplates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="termgenie_rules.xsd">
	<template name="cell_apoptosis">
		<description>Apoptotic process for a cell type</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="celltype" required="true">
				<ontology name="GeneOntology">
					<branch>cell</branch>
				</ontology>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/cl/cell_apoptosis.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>Cell Type Ontology (CL)</category>
			<category>apoptosis</category>
		</categories>
	</template>
	<template name="cell_differentiation">
		<description>Differentiation process for a cell type</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="celltype" required="true">
				<ontology name="GeneOntology">
					<branch>cell</branch>
				</ontology>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref" remoteResource="GO.curators_dbxrefs">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/cl/cell_differentiation.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>Cell Type Ontology (CL)</category>
			<category>differentiation</category>
		</categories>
	</template>
</termgenietemplates>