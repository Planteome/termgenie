		</categories>
	</template>
	<template name="single_multi_organism_process">
		<description>Create a single-organism or multi-organism version for a given biological_process.</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="process" required="true">
				<ontology name="GeneOntology">
					<branch>biological_process</branch>
				</ontology>
				<prefixes preselected="true">
					<prefix>single-organism</prefix>
					<prefix>multi-organism</prefix>
				</prefixes>
