<?xml version="1.0" encoding="UTF-8"?>
<termgenietemplates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="termgenie_rules.xsd">
	<template name="cc_transport_from_to">
		<description>transport or vesicle-mediated transport from cellular component to cellular component</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="from" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
				<prefixes preselected="false">
					<prefix>transport</prefix>
					<prefix>vesicle-mediated transport</prefix>
				</prefixes>
			</field>
			<field name="to" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref" remoteResource="xref">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/cc_transport_from_to.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>biological_process</category>
			<category>transport</category>
		</categories>
	</template>
	<template name="cc_transport">
		<description>transport of a cellular component</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="component" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
				<prefixes preselected="false">
					<prefix>transport</prefix>
					<prefix>vesicle-mediated transport</prefix>
				</prefixes>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref" remoteResource="xref">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/cc_transport.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>biological_process</category>
			<category>transport</category>
		</categories>
	</template>
	<template name="protein_localization_to">
		<description>protein localization and establishment of protein localization to a cellular component</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="component" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
				<prefixes preselected="false">
					<prefix>protein localization</prefix>
					<prefix>establishment of protein localization</prefix>
				</prefixes>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref" remoteResource="xref">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/protein_localization_to.js</ruleFile>
		</ruleFiles>
		<categories>
			<category>biological_process</category>
			<category>localization</category>
		</categories>
	</template>
	<template name="cc_assembly_disassembly">
		<description>assembly or disassembly of a cellular component</description>
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="component" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
