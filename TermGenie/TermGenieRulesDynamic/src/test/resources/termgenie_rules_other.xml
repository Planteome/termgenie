<?xml version="1.0" encoding="UTF-8"?>
<termgenietemplates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="termgenie_rules.xsd">
	<template name="cc_transport_from_to">
		<description>transport or vesicle-medidated transport from cellular component to cellular component</description>
		<ontology name="GeneOntology" />
		<obonamespace>biological_process</obonamespace>
		<fields>
			<field name="from" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
				<prefixes>
					<prefix>transport</prefix>
					<prefix>vesicle-mediated transport</prefix>
				</prefixes>
			</field>
			<field name="to" required="true">
				<ontology name="GeneOntology">
					<branch>cellular_component</branch>
				</ontology>
			</field>
			<field name="Literature_Ref" required="true" />
			<field name="DefX_Ref">
				<cardinality>1..N</cardinality>
			</field>
		</fields>
		<ruleFiles>
			<ruleFile>rules/go/cc_transport_from_to.js</ruleFile>
		</ruleFiles>
	</template>
</termgenietemplates>