<?xml version="1.0" encoding="UTF-8"?>
<ontologyconfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="ontology-configuration.xsd">
	<ontology name="GeneOntology">
