<?xml version="1.0" encoding="UTF-8"?>
<ontologyconfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="ontology-configuration.xsd">
	<ontology name="MP" longName="Phenotype Ontology">
		<source>http://purl.obolibrary.org/obo/mp.owl</source>
		<supports>
			<support>http://purl.obolibrary.org/obo/go.owl</support>
			<support>http://purl.obolibrary.org/obo/uberon.owl</support>
			<support>http://purl.obolibrary.org/obo/cl.owl</support>
			<support>http://purl.obolibrary.org/obo/pato.owl</support>
			<!-- <support>http://purl.obolibrary.org/obo/chebi.owl</support> -->
		</supports>
		<roots>
			<root>MP:0000001</root>		<!-- mammalian phenotype -->
			<root>GO:0003674</root>		<!-- molecular_function -->
			<root>GO:0008150</root> 	<!-- biological_process -->
			<root>GO:0005575</root>		<!-- cellular_component -->
			<root>CL:0000003</root> 	<!-- native cell -->
			<root>UBERON:0001062</root> <!-- anatomical entity -->
			<root>PATO:0000001</root>	<!-- quality -->
			<root>CHEBI:24431</root>	<!-- chemical entity -->
		</roots>
		<ontologybranch name="entities">
			<roots>
				<root>GO:0005575</root> <!-- cellular_component -->
				<root>CL:0000003</root> <!-- native cell -->
				<root>UBERON:0001062</root> <!-- anatomical entity -->
			</roots>
		</ontologybranch>
		<ontologybranch name="occurent">
			<roots>
				<root>GO:0003674</root> <!-- molecular_function -->
				<root>GO:0008150</root> <!-- biological_process -->
			</roots>
		</ontologybranch>
		<ontologybranch name="onset_quality">
			<roots>
				<root>PATO:0002325</root> <!-- onset quality -->
			</roots>
		</ontologybranch>
		<ontologybranch name="default_mp">
			<roots>
				<root>MP:0000001</root> <!-- mammalian phenotype -->
			</roots>
		</ontologybranch>
		<ontologybranch name="chemicals">
			<roots>
				<root>CHEBI:24431</root> <!-- chemical entity -->
			</roots>
		</ontologybranch>
		<ontologybranch name="uberon_location">
			<roots>
				<root>UBERON:0001062</root> <!-- anatomical entity -->
			</roots>
		</ontologybranch>
	</ontology>
</ontologyconfiguration>