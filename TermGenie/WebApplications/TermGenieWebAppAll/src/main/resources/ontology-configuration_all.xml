<?xml version="1.0" encoding="UTF-8"?>
<ontologyconfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="ontology-configuration.xsd">
	<ontology name="GeneOntology">
		<source>http://www.geneontology.org/ontology/obo_format_1_2/gene_ontology_ext.obo
		</source>
		<supports>
			<support>http://www.geneontology.org/scratch/xps/biological_process_xp_self.obo</support>
			<support>http://www.geneontology.org/scratch/xps/biological_process_xp_cellular_component.obo</support>
			<support>http://www.geneontology.org/scratch/xps/molecular_function_xp_protein.obo</support>
			<support>http://www.geneontology.org/scratch/xps/biological_process_xp_uber_anatomy.obo</support>
			<!-- <support>http://www.geneontology.org/scratch/xps/biological_process_xp_plant_anatomy.obo</support> -->
			<support>http://www.geneontology.org/scratch/xps/cellular_component_xp_protein.obo</support>
		</supports>
		<roots>
			<root>GO:0008150</root>
			<root>GO:0003674</root>
			<root>GO:0005575</root>
		</roots>
		<requires>
			<name>ProteinOntology</name>
			<name>Uberon</name>
			<name>PO</name> <!-- Plant ontology -->
		</requires>
		<ontologybranch name="biological_process">
			<roots>
				<root>GO:0008150</root>
			</roots>
		</ontologybranch>
		<ontologybranch name="molecular_function">
			<roots>
				<root>GO:0003674</root>
			</roots>
		</ontologybranch>
		<ontologybranch name="cellular_component">
			<roots>
				<root>GO:0005575</root>
			</roots>
		</ontologybranch>
		<ontologybranch name="macromolecular_complex">
			<roots>
				<root>GO:0032991</root>
			</roots>
		</ontologybranch>
	</ontology>
	<ontology name="ProteinOntology">
		<source>ftp://ftp.pir.georgetown.edu/databases/ontology/pro_obo/pro.obo</source>
		<roots>
			<root>PR:000000001</root>
		</roots>
	</ontology>
	<ontology name="Uberon">
		<source>http://github.com/cmungall/uberon/raw/master/uberon.obo</source>
		<roots>
			<root>UBERON:0001062</root>
		</roots>
	</ontology>
	<ontology name="HumanPhenotype">
		<source>http://compbio.charite.de/svn/hpo/trunk/src/ontology/human-phenotype-ontology.obo</source>
		<supports>
			<support>http://compbio.charite.de/svn/hpo/trunk/src/ontology/human-phenotype-ontology_xp.obo</support>
		</supports>
		<roots>
			<root>HP:0000001</root>
		</roots>
	</ontology>
	<ontology name="FMA" longName="Foundational Model of Anatomy ontology">
		<source>http://obo.svn.sourceforge.net/viewvc/*checkout*/obo/fma-conversion/trunk/fma2_obo.obo</source>
		<roots>
			<root>FMA:62955</root>
		</roots>
	</ontology>
	<ontology name="PATO" longName="Phenotypic Quality Ontology">
		<source>http://pato.googlecode.com/svn/trunk/quality.obo</source>
		<roots>
			<root>PATO:0000001</root>
		</roots>
	</ontology>
	<ontology name="OMP" longName="Mammalian Phenotype Ontology">
		<source>http://obo.cvs.sourceforge.net/*checkout*/obo/obo/ontology/phenotype/mammalian_phenotype.obo</source>
		<roots>
			<root>MP:0000001</root>
		</roots>
	</ontology>
	<ontology name="CL" longName="Cell (Type) Ontology">
		<source>http://obo.cvs.sourceforge.net/*checkout*/obo/obo/ontology/anatomy/cell_type/cell.obo</source>
		<roots>
			<root>CL:0000000</root>
		</roots>
	</ontology>
	<ontology name="PO" longName="Plant ontology">
		<source>http://palea.cgrb.oregonstate.edu/viewsvn/Poc/tags/live/plant_ontology.obo?view=co</source>
		<roots>
			<root>PO:0025131</root>
			<root>PO:0009012</root>
		</roots>
	</ontology>
</ontologyconfiguration>