<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>TermGenie.html</welcome-file>
  </welcome-file-list>

	<servlet>
		<servlet-name>OntologyServiceImpl</servlet-name>
		<servlet-class>org.bbop.termgenie.server.OntologyServiceImpl</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>OntologyServiceImpl</servlet-name>
		<url-pattern>/termgenie/ontology</url-pattern>
	</servlet-mapping>
</web-app>
